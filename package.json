{
  "name": "github-actions-dotnet",
  "version": "0.1.0",
  "description": "GitHub re-usable Actions & Workflows",
  "keywords": [
    "GitHub",
    "actions",
    "workflows",
    "dotnet"
  ],
  "homepage": "https://github.com/percebus/github-actions-dotnet#readme",
  "bugs": {
    "url": "https://github.com/percebus/github-actions-dotnet/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/percebus/github-actions-dotnet.git"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": "JCystems",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "_prettier:xml": "prettier . --plugin=@prettier/plugin-xml",
    "prettier:check:xml": "npm run _prettier:xml -- --check",
    "prettier:write:xml": "npm run _prettier:xml -- --write",
    "_prettier": "prettier .",
    "prettier:check": "npm run _prettier -- --check",
    "postprettier:check": "npm run prettier:check:xml",
    "prettier:write": "npm run _prettier -- --write",
    "postprettier:write": "npm run prettier:write:xml",
    "lint": "npm run prettier:check",
    "format": "npm run prettier:write",
    "postformat": "npm run lint",
    "style": "npm run format",
    "test": "exit 0"
  },
  "devDependencies": {
    "@prettier/plugin-xml": "^3.4.2",
    "prettier": "^3.7.4"
  }
}
